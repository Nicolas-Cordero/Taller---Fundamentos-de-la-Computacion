funciwi sumar(a, b) {
  return a + b;
}

funciwi restar(a, b) {
  return a - b;
}

funciwi multiplicar(a, b) {
  return a * b;
}

funciwi dividir(a, b) {
  return a / b;
}

funciwi potencia(x, y) {
  c = 1;
  whiliwi(y) {
    c = c * x;
    y = y - 1;
  }
  return c;
}

printuwu(sumar(2, 3));
printuwu(restar(10, 4));
printuwu(multiplicar(6, 7));
printuwu(dividir(20, 5));
printuwu(potencia(2, 5));