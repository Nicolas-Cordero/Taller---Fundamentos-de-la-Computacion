funciwi sumar(a, b) {
    returnuwu a + b;
}

funciwi restar(a, b) {
    returnuwu a - b;
}

funciwi multiplicar(a, b) {
    returnuwu a * b;
}

funciwi dividir(a, b) {
    returnuwu a / b;
}

funciwi potencia(x, y) {
    c = 1;
    whilewe (y) {
        c = c * x;
        y = y - 1;
    }
    returnuwu c;
}

funciwi main() {
    printiwi sumar(2, 3);
    printiwi restar(10, 4);
    printiwi multiplicar(6, 7);
    printiwi dividir(20, 5);
    printiwi potencia(2, 5);
    returnuwu 0;
}