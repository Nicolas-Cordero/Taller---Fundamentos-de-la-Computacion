funciwi suma(a, b) {
  printiwi a + b;
}

funciwi resta(a, b) {
  printiwi a - b;
}

funciwi multiplicacion(a, b) {
  printiwi a * b;
}

funciwi division(a, b) {
  printiwi a / b;
}

mainiwi {
  suma(5, 3);             # Imprime 8
  resta(10, 4);           # Imprime 6
  multiplicacion(2, 7);   # Imprime 14
  division(20, 5);        # Imprime 4
}